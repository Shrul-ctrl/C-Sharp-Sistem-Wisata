@model sistem_wisata.Models.Kategori

@{
    ViewData["Title"] = "Details Kategori";
}

<h3>Detail Data Kategori</h3>
<hr />
<div class="card">
    <div class="card-body">
        <div>
            <h4>Kategori</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    Kategori :
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Nama_Kategori)
                </dd>
            </dl>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary btn-sm" data-bs-toggle="tooltip" data-bs-placement="left" title="Edit Data">Edit</a> |
            <a asp-action="Index">Kembali Ke list</a>
        </div>
    </div>
</div>
